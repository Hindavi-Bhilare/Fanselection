
package com.velotech.fanselection.dtos;

public class OfferShareDto {

	private int id;

	private Integer offerRevId;

	private String offerRev;

	private String userMasterLoginId;

	private String userMasterName;

	private Boolean isShare;

	private Boolean isReadOnly;

	private Boolean isEmail;

	private Boolean isSms;

	public int getId() {

		return id;
	}

	public void setId(int id) {

		this.id = id;
	}

	public Integer getOfferRevId() {

		return offerRevId;
	}

	public void setOfferRevId(Integer offerRevId) {

		this.offerRevId = offerRevId;
	}

	public String getOfferRev() {

		return offerRev;
	}

	public void setOfferRev(String offerRev) {

		this.offerRev = offerRev;
	}

	public String getUserMasterLoginId() {

		return userMasterLoginId;
	}

	public void setUserMasterLoginId(String userMasterLoginId) {

		this.userMasterLoginId = userMasterLoginId;
	}

	public String getUserMasterName() {

		return userMasterName;
	}

	public void setUserMasterName(String userMasterName) {

		this.userMasterName = userMasterName;
	}

	public Boolean getIsShare() {

		return isShare;
	}

	public void setIsShare(Boolean isShare) {

		this.isShare = isShare;
	}

	public Boolean getIsReadOnly() {

		return isReadOnly;
	}

	public void setIsReadOnly(Boolean isReadOnly) {

		this.isReadOnly = isReadOnly;
	}

	public Boolean getIsEmail() {

		return isEmail;
	}

	public void setIsEmail(Boolean isEmail) {

		this.isEmail = isEmail;
	}

	public Boolean getIsSms() {

		return isSms;
	}

	public void setIsSms(Boolean isSms) {

		this.isSms = isSms;
	}
}
